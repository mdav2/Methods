import os
import sys
import numpy as np
from HTCCSD import HTCCSD

molecule mol {
    unit bohr
    0 1
    He
    He 1 1.0
    symmetry c1
}

set {
    BASIS         3-21g
    REFERENCE     RHF
    SCF_TYPE      PK
    SOSCF         True
    E_CONVERGENCE 8
    MAXITER       100
    MAX_ATTEMPTS  10
    GUESS         READ
}

X = HTCCSD(mol)

X.HTCCSD('full')
